# PostgreSQL Database Configuration
POSTGRES_USER=your_cpanel_db_user
POSTGRES_PASSWORD=your_cpanel_db_password
POSTGRES_HOST=your_cpanel_db_host
POSTGRES_PORT=5432
POSTGRES_DATABASE=your_cpanel_db_name
POSTGRES_SSL=true

# JWT Authentication
JWT_SECRET=your_jwt_secret_key

# Email Configuration
EMAIL_HOST=your_cpanel_smtp_host
EMAIL_PORT=587
EMAIL_USER=your_cpanel_email
EMAIL_PASSWORD=your_cpanel_email_password
EMAIL_FROM=noreply@yourdomain.com

# Application
NEXT_PUBLIC_APP_URL=https://yourdomain.com
